unit Unit2;

interface

type
  TStudent = class
  private
    FId: Integer;
    FName: string;
    FAge: Integer;
  public
    constructor Create(FName: string; FAge: Integer);
    property Id: string read FId write FId;
    property Name: string read FName write FName;
    property Age: string read FAge write FAge;
    constructor Create; overload;
    constructor Create(FId: Integer; FName: string; FAge: Integer); overload;
  end;

implementation

{ TStudent }

constructor TStudent.Create;
begin
  inherited;

end;

constructor TStudent.Create(FId: Integer; FName: string; FAge: Integer);
begin
  Self.FId := FId;
  Self.FName := FName;
  Self.FAge := FAge;
end;

end.

